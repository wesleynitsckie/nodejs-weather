  "description": "A simple NodeJS example of an Express API interacting with a Weather API",
  "main": "src/app.ts",
  "scripts": {
    "build": "tsc",
    "heroku-postbuild": "npm run build",
    "start": "nodemon",
    "inspect": "nodemon --inspect src/app.ts",
    "test": "jest",
    "lint": "npm run lint:js ",
    "lint:eslint": "eslint --ignore-path .gitignore --ext .ts",
    "lint:js": "npm run lint:eslint src/",
    "lint:fix": "npm run lint:js -- --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wesleynitsckie/nodejs-weather.git"
  },
  "keywords": [
    "nodejs",
    "weather",
    "api",
    "rest",
    "mongodb"
  ],
  "author": "Wesley Nitsckie",
